{
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules",
        "tslint-microsoft-contrib",
        "tslint-consistent-codestyle",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rules": {
        "order-imports": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "jsx-no-lambda": false,
        "member-ordering": false
    }
}